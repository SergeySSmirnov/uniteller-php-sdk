{
	"name" : "rusproj/uniteller-php-sdk",
	"description" : "PHP Library for integration with Uniteller payment processing.",
	"type" : "library",
	"license" : "MIT",
	"authors" : [{
			"name" : "Sergey S. Smirnov",
			"email" : "sergeyssmirnov@mail.ru",
			"homepage" : "https://rusproj.space"
		}
	],
	"require" : {
		"php" : ">=5.5",
		"php-http/httplug" : "^1.1",
		"php-http/guzzle6-adapter" : "^1.1",
		"guzzlehttp/guzzle" : "^6.5"
	},
	"require-dev" : {
		"symfony/var-dumper" : "^3.2",
		"phpunit/phpunit" : "^5"
	},
	"autoload" : {
		"psr-4" : {
			"Rusproj\\Uniteller\\" : "src"
		},
		"files" : [
			"src/helpers.php"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"Rusproj\\Uniteller\\Tests\\" : "tests"
		}
	},
	"scripts" : {
		"test" : "phpunit --coverage-text"
	},
	"support" : {
		"email" : "sergeyssmirnov@mail.ru"
	},
	"keywords" : [
		"Uniteller SDK_v2"
	]
}